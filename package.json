{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@8.10.0",
  "scripts": {
    "build": "hexo clean && hexo generate",
    "clean": "hexo clean",
    "deploy": "hexo deploy",
    "serve": "hexo server --draft",
    "publish": "hexo clean & hexo deploy",
    "postinstall": "patch-package",
    "pub-draft": "node ./plugins/publish-draft/index.js",
    "reserve": "hexo clean && hexo g && hexo s",
    "p": "hexo p",
    "new": "hexo new",
    "create-draft": "hexo new draft",
    "refresh": "rm -rf **/node_modules && pnpm i --offline"
  },
  "hexo": {
    "version": "5.4.2"
  },
  "dependencies": {
    "dayjs": "^1.11.9",
    "hexo": "^5.0.0",
    "hexo-deployer-git": "^4.0.0",
    "hexo-filter-mathjax": "^0.9.0",
    "hexo-generator-archive": "^1.0.0",
    "hexo-generator-baidu-sitemap": "^0.1.9",
    "hexo-generator-category": "^1.0.0",
    "hexo-generator-index": "^2.0.0",
    "hexo-generator-search": "^2.4.3",
    "hexo-generator-sitemap": "^3.0.1",
    "hexo-generator-tag": "^1.0.0",
    "hexo-renderer-ejs": "^1.0.0",
    "hexo-renderer-marked": "^5.0.0",
    "hexo-renderer-pug": "^2.0.0",
    "hexo-renderer-sass": "^0.5.0",
    "hexo-renderer-stylus": "^2.0.0",
    "hexo-server": "^2.0.0",
    "hexo-symbols-count-time": "^0.7.1",
    "hexo-theme-landscape": "^0.0.3",
    "hexo-wordcount": "^6.0.1",
    "pandoc": "^0.2.0"
  },
  "devDependencies": {
    "hexo-browsersync": "^0.3.0",
    "inquirer": "^8.0.0",
    "patch-package": "^6.5.1"
  }
}
